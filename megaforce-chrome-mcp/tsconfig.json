{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022", "DOM"],
    "strict": true,
    "noEmit": true,
    "allowJs": false,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.d.ts"],
  "exclude": ["dist", "node_modules"]
}


